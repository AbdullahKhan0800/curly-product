<script>
    let allProgress = document.querySelectorAll('.P-P');
    let allProgressvalue = document.querySelectorAll('.pp-value');
    $(window).on('scroll', function() {
        if ($(window).scrollTop() >= $('.reached-div').offset().top + $('.reached-div').outerHeight() - window.innerHeight) {
            for(let i = 0 ; i < allProgress.length ; i++ ){
                let widthU = 0;
                const myFunction = () =>{
                    if(widthU >= allProgress[i].getAttribute('data') ){
                        console.log(false)
                        allProgress[i].style.width = allProgress[i].getAttribute('data')
                        clearInterval(myinterval)
                    }else{
                        widthU++
                        allProgress[i].style.width = widthU + "%"
                    }
                }
                const myinterval = setInterval(myFunction , 10)
            }
            for(let i = 0 ; i < allProgressvalue.length ; i++ ){
                let widthUp = 0;
                const myFunctionP = () =>{
                    if(widthUp >= allProgressvalue[i].getAttribute('data') ){
                        console.log(false)
                        allProgressvalue[i].innerHTML = allProgressvalue[i].getAttribute('data') + "%"
                        clearInterval(myinterval)
                    }else{
                        widthUp++
                        allProgressvalue[i].innerHTML = widthUp + "%"
                    }
                }
                const myinterval = setInterval(myFunctionP , 10)
            }
            $('.prodgress-Main').removeClass("reached-div")
            $(window).unbind();
        }
        if ($(window).scrollTop() >= $('.reachtwo').offset().top + $('.reachtwo').outerHeight() - window.innerHeight) {
            for (let i = 0; i < 100; i++) {
                let widthUpp = 0;
                const myFunctionPpp = () => {
                if (widthUpp >= 100) {
                    console.log(false);
                    document.querySelector('.dot-line').style.width = "100%";
                    clearInterval(myintervalpp);
                } else {
                    widthUpp++;
                    document.querySelector('.dot-line').style.width = widthUpp + "%";
                };
                };
                const myintervalpp = setInterval(myFunctionPpp, 100);
            }
            let allHthreeTag = document.querySelectorAll('.innerDivSS h3');
            let timeourTime = 0;
            for (let i = 0; i < allHthreeTag.length; i++) {
                setTimeout(() => {
                    allHthreeTag.forEach(h => {
                        h.style.visibility = 'hidden'
                    })
                    allHthreeTag[i].style.visibility = 'visible'
                }, timeourTime)
                timeourTime += 3200
                console.log(timeourTime)
            }
            $('.main-SCroll').removeClass("reachtwo");
            $('.main-SCroll').unbind();
        }
    });
</script>
