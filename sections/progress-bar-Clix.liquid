<style>
.prodgress-Main{
    display: grid;
    width: min(1050px,90%);
    margin: 40px auto 50px;
    grid-template-columns: 1fr 1fr;
    gap: 30px;
    place-content: center;
}
.mi-l-clix-P-P{
    width: 100%;
    display: flex;
    justify-content: space-evenly;
}
.mi-l-clix-P-P img{
    width:39px;
}
.mi-l-clix-P-P h5{
    font-size: 13px;
}
.left-progress{
    display: grid;
    gap: 20px;
}
.left-progress h3{    
    font-weight: 700;
    font-size: 33px;
    color: #131315;
}
.left-progress p{    
    font-weight: 500;
    color: #131315;
}
.progrss-Pers {
    display: flex;
    gap: 13px;
    justify-self: left;
}

.progrss-Pers h3{
    font-weight: 700;
    font-size: 33px;
    color: #131315;
}
.P-P{
    height: 10px;
    background: linear-gradient(180deg, #F7717D 25.31%, rgba(247, 113, 125, 0.5) 89.26%);
    border-radius: 5px;
}
.P-100{
    width: 1%;
}
.P-98{
    width: 1%;
}
.P-93{
    width: 1%;
}
.P-89{
    width: 1%;
}
.P-86{
    width:1% ;
}
.P-75{
    width: 1%;
}
.progrss-Pers p {
    font-size: 14px;
    display: flex;
    padding-bottom: 9px;
    align-items: end;
    font-weight: 500;
}
.last-acc-iio{
    padding: 7px 20px;
    background: #ABE1ED;
    border: 4px solid #FFFFFF;
    box-shadow: 5px 5px 10px rgb(171 225 237 / 25%);
    backdrop-filter: blur(2px);
    border-radius: 17px;
    color: black;
    font-weight: 600;
    width: fit-content;
    font-size: 18px;
    align-items: center;
    justify-content: center;
    display: flex;
}
.right-progress {
    display: grid;
    gap: 2px;
}
.lastprarpradf{
    font-size: 13px;
    font-weight: 500;
    padding-top: 20px;
}
.res-no{
    display: none;
}
@media screen and (min-width:1600px){
    .prodgress-Main{
        width: min(1550px,90%);
    }
    .prodgress-Main .l-clix h5{
        font-size: 20px !important;
        line-height: 30px;
    }
    .left-progress {
        display: flex;
        gap: 30px;
        flex-direction: column;
    }
    .left-progress h3{
        font-size: 45px !important;
        line-height: 60px;
    }
    .left-progress p{
        font-size: 20px !important;
        line-height: 36px;
    }
    .progrss-Pers p{
        font-size: 20px !important;
        line-height: 50px;
    }
    .progrss-Pers h3{
        font-size: 45px !important;
        line-height: 85px;
        /* or 189% */
    }
    .lastprarpradf{
        font-size: 14px !important;
        line-height: 18px;
    }
    .last-acc-iio{
        font-size: 28px !important;
        line-height: 35px;
    }
}

@media screen and (max-width:1059px){
    .prodgress-Main{
        grid-template-columns: 1fr;
    }
    .res-none{
        display: none;
    }
    .res-no{
        display:flex;
        margin: auto;
    }
}

@media screen and (max-width:590px){
    .left-progress h3{
        font-size:23px;
    }
    .mi-l-clix-P-P{
        display: grid;
        grid-template-columns: 1fr 1fr;
        grid-template-rows: 1fr 1fr;
        gap: 10px;
    }
}
