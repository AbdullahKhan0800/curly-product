<script>
    let slidesOne = document.querySelector('.button-one-multi')  
    let slidesTwo = document.querySelector('.button-two-multi')
    let buttonONe = document.querySelector('#butnOne')
    let buttonTwo = document.querySelector('#butnTwo')
    function SlideOneShow (){
        slidesOne.style.display = 'block';
        slidesTwo.style.display = 'none';
        buttonONe.classList.add('active-btn')
        buttonONe.classList.remove('disable-btn')
        buttonTwo.classList.add('disable-btn')
        buttonTwo.classList.remove('active-btn')
    }
    function SlideTwoShow (){
        slidesOne.style.display = 'none'
        slidesTwo.style.display = 'block'
        buttonTwo.classList.add('active-btn')
        buttonTwo.classList.remove('disable-btn')
        buttonONe.classList.add('disable-btn')
        buttonONe.classList.remove('active-btn')
    }
</script>

