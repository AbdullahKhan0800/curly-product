<style>
    .button-two-multi{
        display: none;
    }
    .main-clix-M-I{
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        margin-top: 50px;
    }
    .main-clix-M-I ul{
        display: flex;
        margin-top: 10px;
        margin-bottom: 30px;
        gap: 20px;        
    }
    .main-clix-M-I ul li{
        display: flex;
        justify-content: left;
        align-items: center;
        margin: 0 0 4px;
        padding: 0;
        font-weight: 500;
        font-family: 'Outfit';
    }
    .main-clix-M-I ul li::before{
        content: url("{{ 'check-mark.svg' | asset_url }}");
        display: flex;
        justify-content: center;
        align-items: center;
        padding: 0;
        height: 0;
        position: relative;
        top: 7px;
        left: 7px;
    }
    .multi-images-buttons{
        display: flex;
        gap:30px;
    }
    .active-btn{
        background: #131315;
        border: 4px solid #FFFFFF;
        box-shadow: 5px 5px 10px rgba(0, 0, 0, 0.25);
        backdrop-filter: blur(2px);
        /* Note: backdrop-filter has minimal browser support */
        color: white;
        font-weight: 700;
        border-radius:18px;
        padding:8px 0;
        width:177px;
        text-align: center;
    }
    .disable-btn{
        background: white;
        border: 4px solid black;
        color: black;
        font-weight: 700;
        border-radius:18px;
        padding:8px 0;
        width:177px;
        text-align: center;
    }
    .multi-content-container{
        width: min(1000px,90%);
    }
    .single-multi-container{
        display: grid;
        grid-template-columns: repeat(2,371px);
        place-content: space-evenly;
        gap: 58px;
        margin:50px 0;
    }
    .rightleft-M{
        display: flex;
        justify-content: center;
        align-items: center;
        gap: 20px;
    }
    .rightleft-M img{
        border-radius: 50%;
            width: 124px;
    }
    .inner-div-clis-M span{
        color: #BEBEBE;
        font-size:14px ;
    }
    .inner-div-clis-M h4{
        font-weight: 500;
        font-size: 19px;
    }
    .inner-div-clis-M{
        display: grid;
    }
    .inner-div-clis-M p{
        font-size: 14px;
    }
    .main-clix-M-I h2{
        font-weight: 700;
        font-size: 40px;
        color: #131315;
    }
    .last-acc-M {
        padding: 7px 20px;
        background: #ABE1ED;
        border: 4px solid #FFFFFF;
        box-shadow: 5px 5px 10px rgb(171 225 237 / 25%);
        backdrop-filter: blur(2px);
        border-radius: 17px;
        color: black;
        font-weight: 600;
        margin: 0px 0 0 0;
        font-size: 18px;
        margin-bottom: 50px;
    }    
    @media screen and (min-width:1600px){
        .main-clix-M-I h2{
            font-size: 62px;
            line-height: 85px;
        }
        .main-clix-M-I ul li{
            font-size: 26px;
            line-height: 30px;
        }
        .active-btn{
            font-size: 28px;
            line-height: 35px;
            padding: 14px 0;
            width: 250px;
        }
        .disable-btn{
            font-size: 28px;
            line-height: 35px;
            padding: 14px 0;
            width: 250px;
        }
        .rightleft-M img{
            width: 185px !important;
        }
        .inner-div-clis-M h4{
            font-size: 35px;
            line-height: 36px;
        }
        .inner-div-clis-M span{
            font-size: 20px;
            line-height: 35px;
        }
        .inner-div-clis-M p{
            font-size: 20px;
            line-height: 35px;
        }
        .single-multi-container{
            grid-template-columns: repeat(2,570px);
            width: min(1530px,90%);
        }
        .main-clix-M-I ul{
            margin-bottom: 70px;
        }
        .last-acc-M{
            font-size: 28px;
            line-height: 35px;
            padding: 11px 35px;
        }
    }
    @media screen and (max-width:818px){
        .main-clix-M-I h2{
            width: 90%;
            margin: auto;
            text-align: center;
            font-size: 31px;
        }
        .single-multi-container{
            grid-template-columns:1fr;
            place-items: baseline;
        }
        .main-clix-M-I ul li{
            font-size:13px;
        }
        .main-clix-M-I ul{
            gap: 0;
        }
    }
    @media screen and (max-width:600px){
        .multi-images-buttons{
            gap: 10px;
        }
        .main-clix-M-I h2{
            font-size: 29px;
        }
    }
</style>
